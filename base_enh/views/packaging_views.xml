<?xml version="1.0" encoding="utf-8"?>
<odoo>  <data>

        <record model="ir.ui.view" id="packaging_list_view">
            <field name="name">Packaging</field>
            <field name="model">packaging</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="name" />
                    <!-- <field name="CBM" /> -->
                    <field name="note"/>
                </tree>
            </field>
        </record>


        <record model="ir.ui.view" id="packaging_form_view">
            <field name="name">Packaging</field>
            <field name="model">packaging</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                            <group>
                                <group>
                                    <field name="name" />
                                    <!-- <field name="CBM" />   -->
                                    <field name="active"/>
                                </group>
                            </group>
                            <group>
                                <field name="note" />
                            </group>
                    </sheet>
                </form>
            </field>
        </record>

        <record model="ir.actions.act_window" id="packaging_action">
            <field name="name">Packaging</field>
            <field name="res_model">packaging</field>
            <field name="view_mode">tree,form</field>
        </record>


        <menuitem  id="packaging_menu"
            parent="location_menu" action="packaging_action"/>

    </data>
</odoo>